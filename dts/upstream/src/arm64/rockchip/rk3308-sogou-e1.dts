/dts-v1/;
#include "rk3308.dtsi"

/ {
	model = "Rockchip RK3308 evb digital-pdm mic v13 board";
	compatible = "rockchip,rk3308-sogou-e1";
	#address-cells = <0x02>;
	#size-cells = <0x02>;
	interrupt-parent = <0x01>;

	dmc {
		compatible = "rockchip,rk3308-dmc";
		clock-names = "dmc_clk";
		operating-points-v2 = <0x0f>;
		clocks = <0x02 0x46>;
		status = "okay";
		center-supply = <0x10>;
		phandle = <0x93>;
	};

	rgb {
		pinctrl-0 = <0x12>;
		compatible = "rockchip,rk3308-rgb";
		status = "okay";
		phandle = <0x96>;
		pinctrl-names = "default";

		ports {
			#address-cells = <0x01>;
			#size-cells = <0x00>;

			port@0 {
				reg = <0x00>;
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				endpoint@0 {
					reg = <0x00>;
					remote-endpoint = <0x0e>;
					phandle = <0x4e>;
				};
			};

			port@1 {
				reg = <0x01>;
				#address-cells = <0x01>;
				phandle = <0x97>;
				#size-cells = <0x00>;

				endpoint@0 {
					reg = <0x00>;
					remote-endpoint = <0x13>;
					phandle = <0x7f>;
				};
			};
		};
	};

	nandc@ff4b0000 {
		reg = <0x00 0xff4b0000 0x00 0x4000>;
		nandc_id = <0x00>;
		interrupts = <0x00 0x51 0x04>;
		compatible = "rockchip,rk-nandc";
		clock-names = "clk_nandc\0hclk_nandc";
		clocks = <0x02 0x2d 0x02 0x99>;
		status = "okay";
		phandle = <0xf8>;
	};

	amba {
		compatible = "arm,amba-bus";
		ranges;
		#address-cells = <0x02>;
		#size-cells = <0x02>;

		dma-controller@ff2c0000 {
			reg = <0x00 0xff2c0000 0x00 0x4000>;
			interrupts = <0x00 0x00 0x04 0x00 0x01 0x04>;
			compatible = "arm,pl330\0arm,primecell";
			clock-names = "apb_pclk";
			peripherals-req-type-burst;
			clocks = <0x02 0x8a>;
			#dma-cells = <0x01>;
			phandle = <0x24>;
		};

		dma-controller@ff2d0000 {
			reg = <0x00 0xff2d0000 0x00 0x4000>;
			interrupts = <0x00 0x02 0x04 0x00 0x03 0x04>;
			compatible = "arm,pl330\0arm,primecell";
			clock-names = "apb_pclk";
			peripherals-req-type-burst;
			clocks = <0x02 0x8b>;
			#dma-cells = <0x01>;
			phandle = <0x2d>;
		};
	};

	motor_vibr {
		compatible = "yt,motor_vibr";
		vibr_gpio-gpios = <0x19 0x10 0x00>;
		status = "okay";
	};

	cpus {
		#address-cells = <0x02>;
		#size-cells = <0x00>;

		idle-states {
			entry-method = "psci";

			cpu-sleep {
				compatible = "arm,idle-state";
				local-timer-stop;
				arm,psci-suspend-param = <0x10000>;
				exit-latency-us = <0xfa>;
				phandle = <0x04>;
				entry-latency-us = <0x78>;
				min-residency-us = <0x384>;
			};
		};

		cpu@0 {
			reg = <0x00 0x00>;
			compatible = "arm,cortex-a35\0arm,armv8";
			cpu-supply = <0x05>;
			enable-method = "psci";
			operating-points-v2 = <0x03>;
			device_type = "cpu";
			cpu-idle-states = <0x04>;
			clocks = <0x02 0x05>;
			#cooling-cells = <0x02>;
			phandle = <0x07>;
			dynamic-power-coefficient = <0x5a>;
		};

		cpu@1 {
			reg = <0x00 0x01>;
			compatible = "arm,cortex-a35\0arm,armv8";
			enable-method = "psci";
			operating-points-v2 = <0x03>;
			device_type = "cpu";
			cpu-idle-states = <0x04>;
			phandle = <0x08>;
		};

		cpu@2 {
			reg = <0x00 0x02>;
			compatible = "arm,cortex-a35\0arm,armv8";
			enable-method = "psci";
			operating-points-v2 = <0x03>;
			device_type = "cpu";
			cpu-idle-states = <0x04>;
			phandle = <0x09>;
		};

		cpu@3 {
			reg = <0x00 0x03>;
			compatible = "arm,cortex-a35\0arm,armv8";
			enable-method = "psci";
			operating-points-v2 = <0x03>;
			device_type = "cpu";
			cpu-idle-states = <0x04>;
			phandle = <0x0a>;
		};
	};

	psci {
		compatible = "arm,psci-1.0";
		method = "smc";
	};

	vccio-sd {
		regulator-name = "vccio_sd";
		gpios = <0x19 0x0f 0x00>;
		compatible = "regulator-gpio";
		regulator-min-microvolt = <0x1b7740>;
		regulator-max-microvolt = <0x325aa0>;
		states = <0x1b7740 0x00 0x325aa0 0x01>;
		phandle = <0x16>;
	};

	pdm-mic-array {
		compatible = "simple-audio-card";
		status = "okay";
		simple-audio-card,name = "rockchip,pdm-mic-array";

		simple-audio-card,codec {
			sound-dai = <0x90>;
		};

		simple-audio-card,cpu {
			sound-dai = <0x8f>;
		};
	};

	ramoops {
		pmsg-size = <0x00 0x00>;
		compatible = "ramoops";
		record-size = <0x00 0x30000>;
		ftrace-size = <0x00 0x00>;
		console-size = <0x00 0xc0000>;
		phandle = <0x95>;
		memory-region = <0x11>;
	};

	pwm@ff160000 {
