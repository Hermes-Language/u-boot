		compatible = "rockchip,rk3308-pwm\0rockchip,rk3328-pwm";
		clock-names = "pwm\0pclk";
		clocks = <0x02 0x1a 0x02 0xce>;
		#pwm-cells = <0x03>;
		status = "disabled";
		phandle = <0xe3>;
